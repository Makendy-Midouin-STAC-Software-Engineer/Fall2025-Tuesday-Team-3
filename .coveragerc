[run]
source = inspections
omit = 
    */migrations/*
    */__pycache__/*
    */venv/*
    */tests/*

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:

partition = true

